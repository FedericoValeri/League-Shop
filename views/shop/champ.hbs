<br> {{#each champion}}
<div class="row">
    <div id="prova" class="col-md-8">
        <div id="prova" class="row">
            <div id="prova" class="col-md-2">

            </div>
            <div id="prova" class="col-md-2">
                <div id="prova" class="row">
                    {{#if_eq role 'Assassino'}}
                    <img width="100" height="100" id="iconRoleBio" src="https://i.imgur.com/vXz3C1X.png"> {{/if_eq}} {{#if_eq role 'Mago'}}
                    <img width="100" height="100" id="iconRoleBio" src="https://i.imgur.com/OZUmpLa.png"> {{/if_eq}} {{#if_eq role 'Tiratore'}}
                    <img width="100" height="100" id="iconRoleBio" src="https://i.imgur.com/hEpbJma.png"> {{/if_eq}} {{#if_eq role 'Combattente'}}
                    <img width="100" height="100" id="iconRoleBio" src="https://i.imgur.com/Y3lEWpZ.png"> {{/if_eq}} {{#if_eq role 'Tank'}}
                    <img width="100" height="100" id="iconRoleBio" src="https://i.imgur.com/kJ7HiDP.png"> {{/if_eq}} {{#if_eq role 'Supporto'}}
                    <img width="100" height="100" id="iconRoleBio" src="https://i.imgur.com/YPXsrns.png"> {{/if_eq}}
                </div>
                <div id="prova" class="row">
                    <h3 align="center" id="roleBio" name="role"> {{role}}</h3>
                </div>
            </div>
            <div id="prova" class="col-md-8">
                <div id="prova" class="row">
                    <h1 id="nameBio"> {{name}}</h1>
                </div>
                <div id="prova" class="row">
                    <h2 id="titleBio"> {{title}}</h2>
                </div>
            </div>
        </div>
        <div id="prova" class="row">
            <h3 id="abilità">Abilità</h3>
        </div>
        {{#each skills}}
        <div id="prova" class="row">
            <div class="col-md-2" id="prova">
                <div id="prova" name="rowImageSkill" class="row">
                    <img width="100" height="100" src="{{imageSrc}}" id="imageSkillBio">
                </div>
            </div>
            <div class="col-md-9">
                <div id="prova" name="rowSkill" class="row">
                    <h3 id="letterTitleBio">{{letter}} - {{title}}</h3>
                    <br>
                    <h5 id="descSkillBio">{{description}}</h5>                    
                </div>
            </div>
            <div class="col-md-1">
                <div id="prova" class="row"></div>
            </div>
        </div>
        {{/each}}
    </div>
    <div id="prova" class="col-md-4">
        <div id="prova" class="row">
            <img width="450" height="450" src="{{imagePath}}" id="imageBio">
        </div>
        <div id="prova" class="row">
            <h5 id="descriptionBio">{{description}}</h5>
        </div>
    </div>
</div>

{{/each}}